<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent">
      <com.zhy.autolayout.AutoRelativeLayout
          android:layout_centerHorizontal="true"
          android:layout_centerVertical="true"
          android:background="@drawable/but_white_yj"
          android:layout_width="402px"
          android:layout_height="211px">
        <TextView
            android:id="@+id/tv_newban"
            android:layout_marginTop="62px"
            android:layout_centerHorizontal="true"
            android:text="发现新版本更新内容简介"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
          <Button
              android:id="@+id/but_dialog_gengxin"
              android:textSize="24px"
              android:layout_marginTop="60px"
              android:textColor="#ffffff"
              android:text="立即更新"
              android:background="@drawable/but_yuanjiao"
              android:layout_below="@id/tv_newban"
              android:layout_centerHorizontal="true"
              android:layout_width="150px"
              android:layout_height="40px"/>
      </com.zhy.autolayout.AutoRelativeLayout>
</com.zhy.autolayout.AutoRelativeLayout>
